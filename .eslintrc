{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "linebreak-style": ["error", "windows"],
        "arrow-parens": "off",
        "comma-dangle": "off",
        "operator-linebreak": "off",
        "indent":"off",
        "require-jsdoc": "off",
        "no-debugger": "off",
        "max-len": ["error", { "code": 880 }]
    },
    "parserOptions": {
        "sourceType": "module"
    }
}